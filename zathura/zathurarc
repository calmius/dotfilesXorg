set selection-clipboard clipboard
set statusbar-h-padding 0
set statusbar-v-padding 0
set page-padding 1
set sandbox none
set guioptions h
set default-bg "#"
set overlay-scrollbar 1

map m scroll half-up
map o scroll half-down
map u scroll up
map e scroll down

map U zoom in
map E zoom out

map r reload
map R rotate
map gg gotop

map f toggle_fullscreen 
